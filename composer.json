{
    "name": "borodulin/router",
    "description": "PSR Router",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Borodulin",
            "email": "borodulin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "psr/simple-cache": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/log": "^1.1",
        "doctrine/annotations": "^1.10",
        "borodulin/finder": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.23",
        "phpunit/phpcov": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Borodulin\\Router\\": "src/",
            "Borodulin\\Http\\": "src/Http/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Borodulin\\Router\\Tests\\": "tests/"
        }
    }
}
